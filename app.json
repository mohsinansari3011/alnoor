{
  "expo": {
    "name": "Al-Noor Grocer",
    "slug": "alnoor",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alnoorgrocer.alnoor",
      "config": {
        "googleMapsApiKey": "<ios maps api key>"
    },"splash": {
      "image": ".assets/splash.png",
      "resizeMode": "cover"
    }
    },"android": {
      "package": "com.alnoorgrocer.alnoor",
      "config": {
        "googleMaps": {
            "apiKey": "AIzaSyACmDuQNCZ_vObZUh7SqPZ4Oa_tuG5_mQk"
        },
        "splash": {
          "mdpi": "./assets/android/splash/splash-mdpi/screen.png",
          "hdpi": "./assets/android/splash/splash-hdpi/screen.png",
          "xhdpi": "./assets/android/splash/splash-xhdpi/screen.png",
          "xxhdpi": "./assets/android/splash/splash-xxhdpi/screen.png",
          "xxxhdpi": "./assets/android/splash/splash-xxxhdpi/screen.png",
          "resizeMode": "cover",
          "backgroundColor": "#ffffff"
        }
    }
    }
  }
}
